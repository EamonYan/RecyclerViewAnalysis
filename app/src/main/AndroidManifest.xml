<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.npe1348.testrecyclerview">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".basicUse.BasicUseActivity" ></activity>
        <activity android:name=".commonAdapterUse.CommonAdapterActivity"></activity>
        <activity android:name=".dragItemAnimatorUse.DragItemAnimatorActivity"></activity>
        <activity android:name=".headerFooterUse.HeaderFooterActivity"></activity>
        <activity android:name=".refreshLoadUse.RefreshLoadActivity"></activity>
    </application>

</manifest>